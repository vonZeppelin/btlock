<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import org.tbee.javafx.scene.layout.fxml.MigPane?>

<MigPane cols="[160]unrel[420]" layout="ins 2, wrap, debug" rows="[25]unrel[150]unrel[]" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label text="BLE Devices" MigPane.cc="growx" />
		<ProgressIndicator MigPane.cc="cell 0 0" /> 
		<TabPane tabClosingPolicy="UNAVAILABLE" MigPane.cc="grow, spany 3"> 
			<tabs>
				<Tab text="One" /> 
				<Tab text="Two" /> 
			</tabs>
		</TabPane>
		<ListView fx:id="devicesList" MigPane.cc="wrap" /> 
		<Button fx:id="connectButton" text="Connect" MigPane.cc="center" /> 
	</children>
</MigPane>
