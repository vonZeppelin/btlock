package org.lbogdanov.btlock.events;

import java.util.*;

public class PeripheralDiscovered {
    public final String name;
    public final UUID uuid;

    public PeripheralDiscovered(UUID uuid, String name) {
        this.uuid = uuid;
        this.name = name;
    }

    @Override
    public String toString() {
        return Optional.ofNullable(name).orElse("Unknown");
    }
}
